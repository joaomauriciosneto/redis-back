{
  "name": "final_back_sem_front",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node ./dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only ./src/index.ts",
    "build": "tsc",
    "postinstall": "tsc",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "npm run typeorm migration:generate -- -d ./src/main/config/typeorm.config.ts",
    "migration:run": "npm run typeorm migration:run -- -d src/main/config/typeorm.config.ts",
    "migration:create": "npm run typeorm migration:create",
    "test": "jest --silent --noStackTrace --runInBand",
    "test:verbose": "jest --verbose --runInBand --detectOpenHandles",
    "test:coverage": "jest --runInBand --coverage",
    "test:watch": "jest --silent --noStackTrace --runInBand --watchAll",
    "test:unit": "jest -c jest.unit.config.ts --silent --noStackTrace --runInBand",
    "test:unit:verbose": "jest -c jest.unit.config.ts --verbose --runInBand --detectOpenHandles",
    "test:unit:coverage": "jest -c jest.unit.config.ts --silent --noStackTrace --runInBand --coverage",
    "test:unit:watch": "jest -c jest.unit.config.ts --silent --noStackTrace --runInBand --watchAll"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/jest": "^29.2.4",
    "@types/supertest": "^2.0.12",
    "@types/typescript": "^2.0.0",
    "@types/uuid": "^8.3.4",
    "ts-jest": "^29.0.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@types/express": "^4.17.14",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "ioredis": "^5.2.4",
    "jest": "^29.3.1",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "supertest": "^6.3.3",
    "typeorm": "^0.3.10",
    "uuid": "^9.0.0"
  }
}
